@{

    Layout = "_adminLayout";
    ViewData["Title"] = "Index";
}
<div class="card card-primary card-outline">
    <div class="card-header">
        <h1>Department</h1>
    </div>
    <div class="card-body">
        <button type="button" class="btn btn-primary mb-3" data-toggle="modal" data-target="#createModal">Create New</button>
        <table class="table table-striped" id="data-departments">
            <thead>
                <tr>
                    <th>No</th>
                    <th>ID</th>
                    <th>Nama</th>
                    <th>Devision ID</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>



<script src="~/js/Department.js"></script>
       